<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="2">
      <listeners>
        <add name="OutlookSynchronization" type="System.Diagnostics.TextWriterTraceListener" initializeData="OutlookSynchronization.log"/>
      </listeners>
    </trace>
    <switches>
      <add name="OutlookSynchronizationSwitch" value="0"/>
    </switches>
  </system.diagnostics>
  <appSettings>
    <!-- This key allows presenting to NAV of custom objects in Outlook Object Model
         Set the value to blank if no customization is required.-->    
    <add key="CustomizationFilePath" value="Customization.xml"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
